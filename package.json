        "laravel-mix": "^2.1.14",
        "lodash": "^4.17.11",
        "popper.js": "^1.12",
        "vue": "^2.1.10",
        "vue-server-renderer": "^2.5.17"
    },
    "dependencies": {
        "startbootstrap-simple-sidebar": "^4.1.1",
        "vue-moment": "^4.0.0",
        "webpack-jquery-ui": "^2.0.1"
    }
